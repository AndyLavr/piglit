[require]
GL ES >= 3.1
GLSL ES >= 3.10

[vertex shader passthrough]

[fragment shader]
#version 310 es

precision mediump float;

struct B { float v; };
struct A { B x, y; };

uniform A thing;

layout(location = 0) out vec4 out_colour;

void
main()
{
	out_colour.x = thing.x.v * thing.y.v;
	out_colour.y = thing.x.v * 0.25;
	out_colour.z = 0.0;
	out_colour.w = 0.0;
}

[test]
clear
draw rect -1 -1 2 2
