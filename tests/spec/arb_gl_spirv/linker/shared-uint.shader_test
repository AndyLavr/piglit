# Test a very simple VS-PS shader pipeline with two uniforms with
# explicit location, using glsl as base.

[require]
GL >= 3.3
GLSL >= 3.30
GL_ARB_gl_spirv

[compute shader]
#version 450

layout (local_size_x = 1, local_size_y = 1, local_size_z = 1) in;

shared uint uishared;

void main()
{
    atomicAdd(uishared, 2);
}


[test]
link success
