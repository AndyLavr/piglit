[require]
GLSL >= 1.50
GL_MAX_VERTEX_ATTRIBS >= 3

[vertex shader]
#version 150

uniform float expected0;
uniform float expected1;

in float value0;
in float value1;

in vec3 piglit_vertex;
out vec4 fs_color;

#define RED vec4(1.0, 0.0, 0.0, 1.0)
#define GREEN vec4(0.0, 1.0, 0.0, 1.0)

void main()
{
    gl_Position = vec4(piglit_vertex, 1.0);
    if (value0 != expected0) {
        fs_color = RED;
	return;
    }
    if (value1 != expected1) {
        fs_color = RED;
	return;
    }
    fs_color = GREEN;
}

[fragment shader]
#version 150

in vec4 fs_color;
out vec4 color;

void main()
{
    color = fs_color;
}

